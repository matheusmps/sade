<form class="filter" id="dev-filter">
        
    <div class="form">
        <div class="row">
            <div class="col-xs-3">
                <label for="phone">Conhecimento</label>
                <select class="form-control" name="skill" id="skill" name="skill" aria-describedby="skill">
                    <option>Conhecimento</option>
                    <option>PHP</option>
                    <option>Javascript</option>
                    <option>Python</option>
                </select>
            </div>

            <div class="col-xs-3">
                <label for="phone">Nível</label>
                <select class="form-control" name="skill" id="skill" name="skill" aria-describedby="skill">
                    <option>Iniciante</option>
                    <option>Intermediário</option>
                    <option>Avançado</option>
                    <option>Experiente</option>
                </select>
            </div>

            <div class="col-xs-2">
                <label for="phone">Disponib. (h)</label>
                <input type="number" placeholder="Disponibilidade" class="form-control" name="availability" id="availability" name="availability" aria-describedby="availability">
            </div>

            <div class="col-xs-3">
                <label for="phone">Nome</label>
                <input type="text" autocomplete="off" placeholder="Nome do dev." class="form-control" name="name" id="name" name="name" aria-describedby="name">
            </div>

            <div class="col-xs-1">
                <label></label>
                <button class="btn btn-default btn-lg btn-main" type="submit">OK</button>
            </div>
        </div>
    </div>
</form>
